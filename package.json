{
  "name": "helenus",
  "version": "0.0.8",
  "description": "A very basic ORM and Client for Cassandra, inspired by 3logic's apollo-cassandra",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "babel": "babel src -d lib",
    "lint": "jshint .",
    "test": "npm run babel && mocha -r jscoverage -R spec --covout=html --coverage=90,75,60 test && open covreporter/index.html",
    "watch": "babel src -d lib -w",
    "prepublish": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalgolani/helenus.git"
  },
  "keywords": [
    "cassandra",
    "client",
    "orm"
  ],
  "author": "kunalgolani@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kunalgolani/helenus/issues"
  },
  "homepage": "https://github.com/kunalgolani/helenus",
  "dependencies": {
    "cassandra-driver": "~2.0.1",
    "es6-promisify": "~1.1.1",
    "underskore": "~0.0.8"
  },
  "devDependencies": {
    "babel": "~5.1.2",
    "chai": "~2.3.0",
    "jscoverage": "~0.5.9",
    "jshint": "~2.6.3",
    "mocha": "~2.2.4"
  }
}
