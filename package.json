{
  "name": "helenus",
  "version": "0.0.16",
  "description": "A very basic ORM and Client for Cassandra, inspired by 3logic's apollo-cassandra",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "babel": "babel src -d lib",
    "lint": "eslint .",
    "precommit": "npm run babel && npm run lint && node --harmony --harmony_arrow_functions ./node_modules/.bin/istanbul cover _mocha",
    "test": "npm run precommit && open ./coverage/lcov-report/index.html",
    "watch": "babel src -d lib -w",
    "prepublish": "npm run precommit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalgolani/helenus.git"
  },
  "publishConfig": {
    "registry": "http://registry.myntra.com:8000/"
  },
  "keywords": [
    "cassandra",
    "client",
    "orm"
  ],
  "author": "kunalgolani@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kunalgolani/helenus/issues"
  },
  "homepage": "https://github.com/kunalgolani/helenus",
  "dependencies": {
    "cassandra-driver": "~2.1.0",
    "es6-promisify": "~1.1.1",
    "underskore": "~0.0.10"
  },
  "devDependencies": {
    "babel": "~5.5.5",
    "babel-eslint": "~3.1.15",
    "babel-istanbul-harmony": "~0.3.13",
    "chai": "~2.3.0",
    "eslint": "~0.22.1",
    "mocha": "~2.2.4"
  }
}
