{
  "name": "helenus",
  "version": "0.0.6",
  "description": "A very basic ORM and Client for Cassandra, inspired by 3logic's apollo-cassandra",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "jshint .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "babel src -d lib -l es6.destructuring,es6.parameters.default,es6.parameters.rest,es6.spread -s -w",
    "prepublish": "npm run lint && babel src -d lib -l es6.destructuring,es6.parameters.default,es6.parameters.rest,es6.spread"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalgolani/helenus.git"
  },
  "keywords": [
    "cassandra",
    "client",
    "orm"
  ],
  "author": "kunalgolani@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kunalgolani/helenus/issues"
  },
  "homepage": "https://github.com/kunalgolani/helenus",
  "dependencies": {
    "cassandra-driver": "^2.0.0",
    "es6-promisify": "^1.1.1",
    "underskore": "^0.0.8"
  },
  "devDependencies": {
    "babel": "^4.7.13",
    "jshint": "^2.6.3"
  }
}
