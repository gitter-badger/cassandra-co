{
  "name": "cassandra-co",
  "version": "0.2.5",
  "description": "A very basic ORM and Client for Cassandra, inspired by 3logic's apollo-cassandra",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "babel": "rm -rf lib/ && babel src -d lib",
    "lint": "eslint .",
    "precommit": "npm run babel && npm run lint && node --harmony --harmony_arrow_functions ./node_modules/.bin/babel-istanbul cover _mocha",
    "test": "npm run precommit && open ./coverage/lcov-report/index.html",
    "watch": "babel src -d lib -w",
    "prepublish": "npm run precommit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalgolani/cassandra-co.git"
  },
  "keywords": [
    "cassandra",
    "client",
    "orm"
  ],
  "author": "kunalgolani@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kunalgolani/cassandra-co/issues"
  },
  "homepage": "https://github.com/kunalgolani/cassandra-co",
  "dependencies": {
    "cassandra-driver": "~2.2.0",
    "es6-promisify": "~3.0.0",
    "lru-cache": "~4.0.0",
    "underskore": "~0.0.10"
  },
  "devDependencies": {
    "babel-cli": "~6.5.0",
    "babel-istanbul": "~0.6.0",
    "babel-plugin-syntax-object-rest-spread": "~6.5.0",
    "babel-plugin-transform-es2015-destructuring": "~6.5.0",
    "babel-plugin-transform-es2015-parameters": "~6.4.0",
    "babel-plugin-transform-object-rest-spread": "~6.5.0",
    "chai": "~3.5.0",
    "eslint": "~2.1.0",
    "mocha": "~2.4.0"
  }
}
